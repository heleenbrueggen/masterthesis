################
# BART package #
################
BART_fit <- wbart(x.train = simdata[[1]][,3:11] %>% as.matrix(), y.train = simdata[[1]]$y)
ggplot(mapping = aes(
x = BART_fit$yhat.train.mean,
y = simdata[[1]]$y
)) + geom_point()
?geom_hline
ggplot(mapping = aes(
x = BART_fit$yhat.train.mean,
y = simdata[[1]]$y
)) +
geom_point() +
theme_minimal() +
geom_abline(slope = 1, intercept = 0, color = 'hotpink')
